.boolean-container {
  margin-top: 16px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 32px;
  padding: 20px 24px;
  background-color: rgb(29, 141, 185);
  border-radius: 14px;
}

.boolean-name {
  flex: 1;
  font-family: PingFangSC-Medium;
  font-size: 16px;
  line-height: 25px;
  color: #110c0c;
}

.van-switch__node {
  background-color: rgb(161, 17, 17);
	box-shadow: 0px 3px 3px 0px 
		rgba(187, 10, 10, 0.05), 
		0px 2px 2px 0px 
		rgba(0, 0, 0, 0.1), 
		0px 2px 2px 0px 
		rgba(0, 0, 0, 0.05);
	border: solid 1px rgba(0, 0, 0, 0.1);
}

